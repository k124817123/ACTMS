<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tooltv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tool:"
        android:textSize="@dimen/sp_20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/guideline5"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/dp_10"/>

    <EditText
        android:id="@+id/tooled"
        android:layout_width="@dimen/dp_100"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@id/tooltv"
        app:layout_constraintTop_toTopOf="@id/tooltv"
        app:layout_constraintLeft_toRightOf="@id/guideline5"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/configtv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:text="Config:"
        android:textSize="@dimen/sp_20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/guideline5"
        app:layout_constraintTop_toBottomOf="@id/tooltv" />

    <EditText
        android:id="@+id/configed"
        android:layout_width="@dimen/dp_100"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/configtv"
        app:layout_constraintBottom_toBottomOf="@id/configtv"
        app:layout_constraintLeft_toRightOf="@id/guideline5"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/pcnametv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:text="PC Name:"
        android:textSize="@dimen/sp_20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/guideline5"
        app:layout_constraintTop_toBottomOf="@id/configtv" />

    <EditText
        android:id="@+id/pcnameed"
        android:layout_width="@dimen/dp_100"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/pcnametv"
        app:layout_constraintBottom_toBottomOf="@id/pcnametv"
        app:layout_constraintLeft_toRightOf="@id/guideline5"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/iptv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:text="IP:"
        android:textSize="@dimen/sp_20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/guideline5"
        app:layout_constraintTop_toBottomOf="@id/pcnametv" />

    <EditText
        android:id="@+id/iped"
        android:layout_width="@dimen/dp_100"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/iptv"
        app:layout_constraintBottom_toBottomOf="@id/iptv"
        app:layout_constraintLeft_toRightOf="@id/guideline5"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/systypetv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:text="System Type:"
        android:textSize="@dimen/sp_20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/guideline5"
        app:layout_constraintTop_toBottomOf="@id/iptv" />

    <EditText
        android:id="@+id/systypeed"
        android:layout_width="@dimen/dp_100"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/systypetv"
        app:layout_constraintBottom_toBottomOf="@id/systypetv"
        app:layout_constraintLeft_toRightOf="@id/guideline5"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/mactv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:text="MAC:"
        android:textSize="@dimen/sp_20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/guideline5"
        app:layout_constraintTop_toBottomOf="@id/systypetv" />

    <EditText
        android:id="@+id/maced"
        android:layout_width="@dimen/dp_100"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/mactv"
        app:layout_constraintBottom_toBottomOf="@id/mactv"
        app:layout_constraintLeft_toRightOf="@id/guideline5"
        app:layout_constraintRight_toRightOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />
</androidx.constraintlayout.widget.ConstraintLayout>